<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.android.batteryreport">
    <uses-permission android:name="android.permission.STATUS_BAR"/>
    <uses-permission android:name="android.permission.EXPAND_STATUS_BAR"/>
    <uses-permission android:name="android.permission.DEVICE_POWER"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".BattTempCooldownScreen">
            <intent-filter>
                <action android:name="com.android.batteryreport.TEMPERATURE_COOLDOWN_ACTION"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>
</manifest>
